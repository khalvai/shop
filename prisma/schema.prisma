
generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @default(autoincrement()) @id
  email    String? @unique
  name     String?
  lastname String?
  phone    String?
  address  String?
  createdAt  DateTime     @default(now())
 password  String?
}

